created() {\n    console.log(this.favorites);\n    axios.get(\"http://localhost:3000/movies\").then((get_response) => {\n      console.log(get_response);\n      this.favorites = get_response.data;\n    });\n  },\n};\n</script>\n<style>\nboxx{\n\n  background-color: green;\n  opacity: 0.3;\n}\n\n</style>"],"sourceRoot":""}]}